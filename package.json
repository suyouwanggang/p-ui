{
  "name": "@suyouwanggang/p-ui",
  "version": "1.0.1",
  "main": "index.ts",
  "config": {
    "themeColor": "#e74e0f"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "Web Components implementation by lit-element ",
  "repository": "git@github.com:suyouwanggang/p-ui.git",
  "dependencies": {
    "element-closest-polyfill": "^1.0.2",
    "lit-element": "^2.4.0",
    "number-precision": "^1.5.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@types/node": "12.0.2",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.0",
    "fs-extra": "8.0.1",
    "gh-pages": "2.0.1",
    "html-webpack-plugin": "^4.4.1",
    "jest": "24.8.0",
    "lit-scss-loader": "^1.0.1",
    "node-sass": "^4.14.1",
    "polymer-webpack-loader": "2.0.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "to-string-loader": "^1.1.5",
    "ts-jest": "24.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.9.7",
    "typescript-require": "^0.3.0",
    "web-component-analyzer": "^1.1.6",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "scripts": {
    "analyze": "wca analyze \"components/**/*.ts\" --outFile custom-elements.json",
    "analyze2": "wca analyze \"components/**/*.ts\" --format markdown --outFile custom-elements.md",
    "vscode": "wca analyze \"components/**/*.ts\" --format vscode --outFile custom-elements.vscode",
    "copywc": "cp -r iconfont dist && cp -r node_modules/@webcomponents/webcomponentsjs/bundles dist && cp node_modules/@webcomponents/webcomponentsjs/webcomponents-loader.js dist",
    "create": "ts-node ./scripts/generator.ts",
    "dev": "npx webpack --watch",
    "start": " webpack-dev-server --open --mode development",
    "build": " npx webpack --mode production ",
    "build:clean": "rm -rf ./dist"
  },
  "author": "suyouwanggang",
  "keywords": [
    "web components",
    "custom elements",
    "p-ui",
    "lit-element",
    "lit-html",
    "polymer lit-element",
    "p-tips",
    "p-icon",
    "p-input",
    "p-botton",
    "p-checkbox",
    "p-checkbox-group",
    "p-radio",
    "p-radio-group",
    "p-rate",
    "p-tab",
    "p-tab-content",
    "p-tree",
    "p-tree-node"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register"
    ]
  },
  "license": "ISC"
}
