{
  "name": "p-ui",
  "version": "0.0.0",
  "description": "Webcomponent p-ui following open-wc recommendations",
  "repository": "git@github.com:suyouwanggang/p-ui.git",
  "author": "p-ui",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "analyze": "wca analyze \"components/**/*.ts\" --outFile custom-elements.json",
    "analyzeMd": "wca analyze \"components/**/*.ts\" --outFile custom-elements.md",
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index demo/index.html --node-resolve --open --watch\"",
    "tsc:watch": "tsc --watch",
    "css:watch": "node watch.css.js",
    "lint:eslint": "eslint --ext .ts,.html . --ignore-path .gitignore ./dist",
    "format:eslint": "eslint --ext .ts,.html . --fix --ignore-path .gitignore ./dist",
    "lint:prettier": "prettier \"**/*.js\" \"**/*.ts\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.js\" \"**/*.ts\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:eslint && npm run format:prettier",
    "test": "tsc && web-test-runner --coverage",
    "test:watch": "web-test-runner --watch",
    "storybook": "concurrently --kill-others --names tsc,storybook \"npm run tsc:watch\" \"start-storybook --node-resolve --watch --open\"",
    "storybook:build": "build-storybook",
    "build": "rm -rf dist && tsc && rollup -c rollup.config.js",
    "start:build": "web-dev-server --root-dir ./ --app-index index.html --node-resolve --open  --watch",
    "prepublish": "tsc"
  },
  "dependencies": {
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "number-precision": "^1.5.0"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^1.0.0",
    "@open-wc/demoing-storybook": "^2.0.0",
    "@open-wc/eslint-config": "^2.0.0",
    "@open-wc/testing": "^2.0.0",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "@web/dev-server": "^0.0.12",
    "@web/test-runner": "^0.7.41",
    "concurrently": "^5.1.0",
    "deepmerge": "^4.2.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.4",
    "rimraf": "^2.6.3",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^2.0.0",
    "tslib": "^1.11.0",
    "typescript": "~4.0.3",
    "uglifycss": "^0.0.29"
  },
  "keywords": [
    "web components",
    "custom elements",
    "p-ui",
    "lit-element",
    "lit-html",
    "polymer lit-element",
    "p-tips",
    "p-icon",
    "p-input",
    "p-botton",
    "p-checkbox",
    "p-checkbox-group",
    "p-radio",
    "p-radio-group",
    "p-rate",
    "p-tab",
    "p-tab-content",
    "p-tree",
    "p-tree-node"
  ],
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
